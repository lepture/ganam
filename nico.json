{
  "source": "docs",
  "output": "_site",
  "permalink": "{{directory}}/{{filename}}",
  "sitename": "Ganam Style",
  "google": "UA-21475122-5",
  "ganam": {
    "source": "./docs/guide",
    "output": "guide",
    "options": {
      "paths": ["docs/guide", "node_modules/nib/lib"]
    }
  },
  "writers": [
    "nico.PageWriter",
    "nico.FileWriter",
    "nico.StaticWriter",
    "nico.GanamWriter"
  ]
}
